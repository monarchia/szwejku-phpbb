{% set T_FAVICON_PATH = T_THEME_PATH ~ "/images/favicon" %}
{% block favicons %}
<link rel="apple-touch-icon" sizes="180x180" href=" {{ T_FAVICON_PATH ~ "/apple-touch-icon.png?assets_version=" ~ T_ASSETS_VERSION }}">
<link rel="icon" type="image/png" sizes="32x32" href="{T_FAVICON_PATH}/favicon-32x32.png?assets_version={T_ASSETS_VERSION}">
<link rel="icon" type="image/png" sizes="16x16" href="{T_FAVICON_PATH}/favicon-16x16.png?assets_version={T_ASSETS_VERSION}">
<link rel="manifest" href="{T_FAVICON_PATH}/site.webmanifest?assets_version={T_ASSETS_VERSION}">
<link rel="mask-icon" href="{T_FAVICON_PATH}/safari-pinned-tab.svg?assets_version={T_ASSETS_VERSION}" color="#fcc80f">
<link rel="shortcut icon" href="{T_FAVICON_PATH}/favicon.ico?assets_version={T_ASSETS_VERSION}">
<meta name="msapplication-TileColor" content="#ffcc00">
<meta name="theme-color" content="#161616">
{% endblock favicons %}